<h1>Listing launchers</h1>

<table>
  <tr>
  </tr>

<% for launcher in @launchers %>
  <tr>
    <td><%= link_to 'Show', launcher %></td>
    <td><%= link_to 'Edit', edit_launcher_path(launcher) %></td>
    <td><%= link_to 'Destroy', launcher, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

