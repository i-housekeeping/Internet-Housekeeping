<h1>Listing dialogs</h1>

<table>
  <tr>
  </tr>

<% for dialog in @dialogs %>
  <tr>
    <td><%= link_to 'Show', dialog %></td>
    <td><%= link_to 'Edit', edit_dialog_path(dialog) %></td>
    <td><%= link_to 'Destroy', dialog, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New dialog', new_dialog_path %>
